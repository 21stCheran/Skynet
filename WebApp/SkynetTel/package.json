{
  "name": "skynettel",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run bridge\" \"npm run client\" --names \"🌉BRIDGE,⚛️REACT\" --prefix-colors \"cyan,magenta\" --kill-others-on-fail",
    "client": "vite",
    "bridge": "node websocket-udp-bridge.js",
    "dev:client": "npm run client",
    "dev:bridge": "npm run bridge",
    "start": "npm run dev",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "clean": "rm -rf dist node_modules/.vite",
    "test:bridge": "node -e \"console.log('Testing WebSocket bridge...'); import('./websocket-udp-bridge.js');\""
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.2",
    "@mui/material": "^7.3.2",
    "lucide-react": "^0.544.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "concurrently": "^9.2.1",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "vite": "^7.1.7"
  }
}
